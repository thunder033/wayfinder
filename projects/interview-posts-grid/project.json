{
  "name": "interview-posts-grid",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "projects/interview-posts-grid/src",
  "tags": ["scope:app"],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/projects/interview-posts-grid",
        "index": "projects/interview-posts-grid/src/index.html",
        "browser": "projects/interview-posts-grid/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "projects/interview-posts-grid/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "projects/interview-posts-grid/public"
          }
        ],
        "styles": ["projects/interview-posts-grid/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "interview-posts-grid:build:production"
        },
        "development": {
          "buildTarget": "interview-posts-grid:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "interview-posts-grid:build"
      }
    },
    "test": {
      "options": {
        "reportsDirectory": "../coverage/interview-posts-grid"
      }
    },
    "lint": {},
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "interview-posts-grid:build",
        "staticFilePath": "dist/projects/interview-posts-grid/browser",
        "spa": true
      }
    }
  }
}
